:series><sb:title><sb:maintitle>Polymer</sb:maintitle></sb:title><sb:volume-nr>24</sb:volume-nr></sb:series><sb:issue-nr>10</sb:issue-nr><sb:date>1983</sb:date></sb:issue><sb:pages><sb:first-page>1341</sb:first-page><sb:last-page>1354</sb:last-page></sb:pages></sb:host></sb:reference></ce:bib-reference><ce:bib-reference id="bib6"><ce:label>[6]</ce:label><sb:reference id="sref6"><sb:contribution langtype="en"><sb:authors><sb:author><ce:given-name>J.P.</ce:given-name><ce:surname>Pascault</ce:surname></sb:author><sb:author><ce:given-name>R.J.J.</ce:given-name><ce:surname>Williams</ce:surname></sb:author></sb:authors><sb:title><sb:maintitle>Formulation and characterization of thermoset-thermoplastic blends</sb:maintitle></sb:title></sb:contribution><sb:host><sb:edited-book><sb:book-series><sb:editors><sb:editor><ce:given-name>D.R.</ce:given-name><ce:surname>Paul</ce:surname></sb:editor><sb:editor><ce:given-name>C.B.</ce:given-name><ce:surname>Bucknall</ce:surname></sb:editor></sb:editors><sb:series><sb:title><sb:maintitle>Polymer blends</sb:maintitle></sb:title><sb:volume-nr>vol. 1</sb:volume-nr></sb:series></sb:book-series><sb:date>1999</sb:date><sb:publisher><sb:name>Wiley</sb:name><sb:location>New York</sb:location></sb:publisher></sb:edited-book><sb:pages><sb:first-page>379</sb:first-page><sb:last-page>415</sb:last-page></sb:pages></sb:host></sb:reference></ce:bib-reference><ce:bib-reference id="bib7"><ce:label>[7]</ce:label><sb:reference id="sref7"><sb:contribution langtype="en"><sb:authors><sb:author><ce:given-name>C.B.</ce:given-name><ce:surname>Bucknall</ce:surname></sb:author><sb:author><ce:given-name>I.K.</ce:given-name><ce:surname>Partridge</ce:surname></sb:author></sb:authors></sb:contribution><sb:host><sb:issue><sb:series><sb:title><sb:maintitle>Polymer</sb:maintitle></sb:title><sb:volume-nr>24</sb:volume-nr></sb:series><sb:issue-nr>5</sb:issue-nr><sb:date>1983</sb:date></sb:issue><sb:pages><sb:first-page>639</sb:first-page><sb:last-page>644</sb:last-page><