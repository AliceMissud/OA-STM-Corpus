b:authors><sb:author><ce:given-name>S.R.</ce:given-name><ce:surname>Arwade</ce:surname></sb:author><sb:author><ce:given-name>M.A.</ce:given-name><ce:surname>Lackner</ce:surname></sb:author><sb:author><ce:given-name>M.D.</ce:given-name><ce:surname>Grigoriu</ce:surname></sb:author></sb:authors><sb:title><sb:maintitle>Probabilistic models for wind turbine and wind farm performance</sb:maintitle></sb:title></sb:contribution><sb:host><sb:issue><sb:series><sb:title><sb:maintitle>J Solar Energy Eng</sb:maintitle></sb:title><sb:volume-nr>133</sb:volume-nr></sb:series><sb:issue-nr>4</sb:issue-nr><sb:date>2011</sb:date></sb:issue><sb:pages><sb:first-page>041006</sb:first-page><sb:last-page>041009</sb:last-page></sb:pages></sb:host></sb:reference></ce:bib-reference><ce:bib-reference id="bib10"><ce:label>[10]</ce:label><sb:reference id="sref10"><sb:contribution langtype="en"><sb:authors><sb:author><ce:surname>PÃ¶yry Energy Consulting</ce:surname></sb:author></sb:authors><sb:title><sb:maintitle>Implications of intermittency</sb:maintitle></sb:title></sb:contribution><sb:host><sb:book><sb:date>2009</sb:date></sb:book></sb:host><sb:comment>Oxford. URL:</sb:comment><sb:host><sb:e-host><ce:inter-ref xlink:href="http://www.uwig.org/ImpactofIntermittency.pdf" id="intref0030" xlink:type="simple">http://www.uwig.org/ImpactofIntermittency.pdf</ce:inter-ref></sb:e-host></sb:host></sb:reference></ce:bib-reference><ce:bib-reference id="bib11"><ce:label>[11]</ce:label><sb:reference id="sref11"><sb:contribution langtype="en"><sb:authors><sb:author><ce:surname>Sinclair Knight Merz</ce:surname></sb:author></sb:authors><sb:title><sb:maintitle>Growth scenarios for UK renewables generation and implications for future developments and operation of electricity networks</sb:maintitle></sb:title></sb:contribution><sb:host><sb:book><sb:date>2008</sb:date></sb:book></sb:host><sb:comment>URN 08/1021</sb:comment></sb:reference></ce:bib-reference><ce:bib-reference id="bib12"><ce:label>[12]</ce:label><sb:re